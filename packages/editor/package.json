{
  "name": "@sera/editor",
  "version": "0.1.1",
  "description": "An Electron application with React and TypeScript",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "type": "module",
  "main": "./dist/main/index.js",
  "license": "ISC",
  "scripts": {
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --watch",
    "setup": "rm -rf ./out/* && yarn typecheck:node && yarn typecheck:web && electron-vite build",
    "build": "yarn setup && yarn bundle && node ./scripts/build.js",
    "bundle": "node electron/remotion/bundleRemotion.js"
  },
  "dependencies": {
    "@radix-ui/themes": "^3.1.6",
    "@reduxjs/toolkit": "^2.5.0",
    "@remotion/bundler": "^4.0.241",
    "@remotion/media-parser": "^4.0.241",
    "@remotion/player": "^4.0.241",
    "@remotion/renderer": "^4.0.241",
    "electron-log": "^5.2.4",
    "elevenlabs": "^1.50.2",
    "lodash-es": "^4.17.21",
    "react-redux": "^9.2.0",
    "redux-logger": "^3.0.6",
    "remotion": "^4.0.241",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@heroicons/react": "^2.2.0",
    "@stitches/react": "^1.2.8",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/redux-logger": "^3.0.13",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "33.2.1",
    "electron-builder": "^25.1.8",
    "electron-vite": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^5.3.1"
  }
}
